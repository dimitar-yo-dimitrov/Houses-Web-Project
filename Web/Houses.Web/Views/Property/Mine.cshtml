@model IEnumerable<PropertyServiceViewModel>

@{
    ViewBag.Title = "My Properties";
}

<h2 class="text-center">@ViewBag.Title</h2>
<hr />

@if (Model.Any())
{
    <div class="row">
        @foreach (var property in Model)
        {
            <partial name="_MinePropertyPartial" model="@property" />
        }
    </div>
}
else
{
    <h3 class="text-center">You have no houses yet!</h3>
}

@section Scripts {
    <partial name="_ValidationScriptsPartial" />
}